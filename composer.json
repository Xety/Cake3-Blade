{
  "name": "xety/cake3-blade",
  "type": "cakephp-plugin",
  "description": "Cake3 plugin for Blade templating.",
  "keywords": ["cakephp", "cake3", "plugin", "blade", "templating", "laravel"],
  "homepage": "https://github.com/Xety/Cake3-Blade",
  "license": "MIT",
  "support":{
    "source":"https://github.com/Xety/Cake3-Blade",
    "issues":"https://github.com/Xety/Cake3-Blade/issues"
  },
  "authors": [
    {
      "name": "Xety",
      "email": "zoro.fmt@gmail.com",
      "homepage": "https://github.com/Xety"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "cakephp/cakephp": "~3.0",
    "illuminate/view": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "Xety\\Cake3Blade\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xety\\Cake3Blade\\Test\\": "tests"
    }
  }

}